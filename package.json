  "devDependencies": {
    "autoprefixer": "^9.1.0",
    "node-sass": "^4.9.2",
    "npm-run-all": "^4.1.3",
    "postcss-cli": "^6.0.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/therealdylanmartin/wbtp.git"
  },
